<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Media_Player_WPF.Styles"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    
    <!-- http://fontawesome.io/cheatsheet/ -->
    <!-- https://demo.openinnovation.it/cdn/ -->
    <FontFamily x:Key="FontAwesome_Solid">pack://application:,,,/Fonts/#Font Awesome 5 Pro Solid</FontFamily>
    <FontFamily x:Key="FontAwesome_Regular">pack://application:,,,/Fonts/#Font Awesome 5 Pro Regular</FontFamily>
    <FontFamily x:Key="FontAwesome_Light">pack://application:,,,/Fonts/#Font Awesome 5 Pro Light</FontFamily>
    <FontFamily x:Key="FontAwesome_Brands">pack://application:,,,/Fonts/#Font Awesome 5 Brands Regular</FontFamily>

    <!-- FontAwesom Icons -->
    <system:String x:Key="FontAwesome_VolumeMute">&#xf2e2;</system:String>
    <system:String x:Key="FontAwesome_VolumeUp">&#xf028;</system:String>
    <system:String x:Key="FontAwesome_Play">&#xf04b;</system:String>
    <system:String x:Key="FontAwesome_Pause">&#xf04c;</system:String>
</ResourceDictionary>